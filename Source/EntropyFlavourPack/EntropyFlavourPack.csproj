            <Private>False</Private>
        </Reference>
        <Reference Include="VFETribals">
            <HintPath Condition="Exists('../../../3079786283')">../../../3079786283/1.6/Assemblies/VFETribals.dll</HintPath>
            <HintPath Condition="Exists('../../../../../../workshop/content/294100/3079786283')"
